+	pci_io_resource.start  = LOONGSON_PCI_IO_START;
+	pci_io_resource.end    = 0x3ffff;
+	ioport_resource.end    = 0xfffff;
+	virtio_pci_controller.io_map_base = mips_io_port_base;
+	register_pci_controller(&virtio_pci_controller);
+}
+
